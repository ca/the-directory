                            <p>{{ static.step-one-description }}</p>
                        </div>
                    </div>
                    <div class="col-12 col-md-4 text-center wow fadeInUp mb-5 mb-md-0">
                        <div class="icon-box box-line box-line-dotted2 lis-relative"> <img src="https://cdn.turbo360-vertex.com/lister-startup-ij64jh/public/dist/images/icon-2.png" alt="" class="img-fluid mb-4 z-index-99  lis-relative" />
                            <h5>{{ static.step-two }}</h5>
                            <p>{{ static.step-two-description }}</p>
                        </div>
                    </div>
                    <div class="col-12 col-md-4 text-center wow fadeInUp mb-5 mb-md-0">
                        <div class="icon-box"> <img src="https://cdn.turbo360-vertex.com/lister-startup-ij64jh/public/dist/images/icon-3.png" alt="" class="img-fluid mb-4 z-index-99  lis-relative" />
                            <h5>{{ static.step-three }}</h5>
                            <p>{{ static.step-three-description }}</p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!--End Work -->

        <!-- Testimonial -->
        <section class="lis-bg-light">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-12 col-md-6 text-center">
                        <div id="testimonial" class="owl-carousel">
                            <div class="item text-center">
                                <div class="quote"><img src="https://cdn.turbo360-vertex.com/lister-startup-ij64jh/public/dist/images/quote.png" alt="" class="img-fluid mx-auto lis-relative" /></div>
                                <h5>"{{static.testimonial-title}}"</h5>
                                <p class="lis-line-height-2 lis-dark">{{static.testimonial-body}}</p>
                                <div class="author">
                                    <A href="#"><img src="{{static.testimonial-image}}" alt="" class="img-fluid mx-auto rounded-circle py-3" /></A>
                                    <h5 class="lis-primary mb-2">{{static.testimonial-name}}</h5> <span>{{static.testimonial-company}}</span> </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
